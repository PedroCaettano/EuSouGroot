#ifndef BINARYTREE_H
#define BINARYTREE_H
class BinaryTree{
    public:
    BinaryTree();
    //~BinaryTree();
    void PreOrder();
    void InOrder();
    void PostOrder();
    int QtdVertices();


    private:
    struct TreeNode{
        char Entry;
        TreeNode* LeftNode;
        TreeNode* RightNode;
    };
    typedef TreeNode* TreePointer;
    TreePointer root;
    void PreOrder (TreePointer r);
    void InOrder (TreePointer r);
    void PostOrder (TreePointer r);
    int QtdVertive
};
#endif
